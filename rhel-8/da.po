msgstr "%(product)s %(version)s til %(arch)s"

#: pyanaconda/storage/osinstall.py:357
#, python-format
msgid "%(product)s Linux %(version)s for %(arch)s"
msgstr "%(product)s Linux %(version)s til %(arch)s"

